        "import requests\n",
        "\n",
        "def validar_api(url, headers=None):\n",
        "    print(f\"\\nüîç Validando API: {url}\")\n",
        "\n",
        "    # 1. Verifica criptografia em tr√¢nsito (HTTPS)\n",
        "    if url.startswith(\"https://\"):\n",
        "        print(\" Criptografia em tr√¢nsito: HTTPS detectado\")\n",
        "    else:\n",
        "        print(\"‚ùå Criptografia em tr√¢nsito: URL n√£o usa HTTPS\")\n",
        "\n",
        "    # 2. Verifica autentica√ß√£o\n",
        "    if headers and \"Authorization\" in headers:\n",
        "        print(\" Autentica√ß√£o: Header 'Authorization' presente\")\n",
        "    else:\n",
        "        print(\" Autentica√ß√£o: Header 'Authorization' ausente ou n√£o informado\")\n",
        "\n",
        "    # 3. Tipo da API (REST ou GraphQL Web, SOAP, Hardware ou Plataforma ‚Äî simples heur√≠stica)\n",
        "    try:\n",
        "        resp = requests.get(url, headers=headers, timeout=5)\n",
        "        content_type = resp.headers.get(\"Content-Type\", \"\")\n",
        "\n",
        "        if \"/graphql\" in url or \"graphql\" in resp.text.lower():\n",
        "            print(\"üîé Tipo de API: Possivelmente GraphQL\")\n",
        "        elif \"application/json\" in content_type:\n",
        "            print(\"üîé Tipo de API: Possivelmente REST (JSON)\")\n",
        "        else:\n",
        "            print(\"üîé Tipo de API: Tipo n√£o identificado com precis√£o\")\n",
        "    except Exception as e:\n",
        "        print(f\"‚ùå Erro ao acessar a API: {e}\")\n",
        "\n",
        "# üîß EXEMPLO DE USO\n",
        "url_da_api = \"\"\n",
        "headers = {\n",
        "    # Exemplo de autentica√ß√£o\n",
        "    # \"Authorization\": \"Bearer SEU_TOKEN_AQUI\"\n",
        "}\n",
        "\n",
        "validar_api(url_da_api, headers)"
      ]
